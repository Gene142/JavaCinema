<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Button fx:id="clickme" layoutX="155.0" layoutY="202.0" mnemonicParsing="false" onAction="#Login" prefHeight="45.0" prefWidth="193.0" text="Login" />
      <Label layoutX="199.0" layoutY="57.0" text="ODEON Cinemas" />
      <TextField fx:id="txtUsername" layoutX="168.0" layoutY="104.0" promptText="Enter Username" />
      <TextField fx:id="txtPassword" layoutX="167.0" layoutY="148.0" promptText="Enter Password" />
      <Button fx:id="SignUp" layoutX="217.0" layoutY="335.0" mnemonicParsing="false" onAction="#SignUp" text="Sign Up" />
      <Label fx:id="loginMessage" layoutX="114.0" layoutY="278.0" prefHeight="45.0" prefWidth="293.0" text="Please Enter Your Username and Password" textAlignment="CENTER" />
   </children>
</AnchorPane>
